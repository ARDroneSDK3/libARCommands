#! /bin/sh

if [ -e Makefile ] ; then
	make clean
fi

#Python generated C/H files
rm -rf $(python ../Xml/generateCommandsList.py -fname)
#Python generated Directories (if empty)
PDIR=$(python ../Xml/generateCommandsList.py -dname)
for DIR in $PDIR; do
	rmdir $DIR 2>/dev/null
done

# Generated files.
rm -f \
    INSTALL \
    Makefile \
    Makefile.in \
    aclocal.m4 \
    config.h \
    config.h.in \
    config.h.in~ \
    config.log \
    config.status \
    configure \
    libtool \
    stamp-h1

find .. -name '*~' -delete

# Generated directories.
rm -rf \
	doxygen-doc \
    autom4te.cache \
    *DSYM* \
    config \
    m4 \
    libNetWork_dbg.la \
    libNetWork.la

find .. -name '.deps' -exec rm -rf {} \;
find .. -name '.dirstamp' -exec rm -rf {} \;

